{
  "name": "agra-ranker",
  "description": "Web server to rank greyhounds",
  "version": "6.3.0",
  "private": false,
  "license": "MIT",
  "author": "Scott Brown",
  "repository": {
    "type": "git",
    "url": "https://github.com/link1900/agra-ranker.git"
  },
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "scripts": {
    "start": "node node_modules/gulp/bin/gulp",
    "dev": "npm start",
    "lint": "node ./node_modules/eslint/bin/eslint.js ./app ./client ./specs",
    "lint-fix": "node ./node_modules/eslint/bin/eslint.js --fix ./app ./client ./specs",
    "test": "npm run lint && npm run mocha",
    "test-one": "mocha --compilers js:babel-core/register --require scripts/mochaBootLoader.js",
    "mocha": "mocha --compilers js:babel-core/register --require scripts/mochaBootLoader.js ./app/**/*Test*.js ./specs/*.js",
    "postinstall": "node node_modules/bower/bin/bower install && node node_modules/gulp/bin/gulp js",
    "script": "babel-node"
  },
  "dependencies": {
    "JSONStream": "1.0.4",
    "angular-auth0": "3.0.0",
    "auth0-js": "9.13.2",
    "babel-cli": "6.24.1",
    "babel-preset-env": "1.4.0",
    "babel-preset-stage-0": "6.24.1",
    "bower": "1.8.8",
    "browser-sync": "2.23.6",
    "csv": "1.1.1",
    "dataloader": "1.3.0",
    "del": "^3.0.0",
    "dotenv": "4.0.0",
    "express": "3.4.8",
    "express-jwt": "6.0.0",
    "gulp": "3.9.1",
    "gulp-angular-templatecache": "1.9.1",
    "gulp-concat": "2.6.0",
    "gulp-nodemon": "2.2.1",
    "highland": "2.10.5",
    "jsonwebtoken": "7.1.9",
    "lodash": "4.17.21",
    "moment-timezone": "0.5.14",
    "mongoose": "5.7.5",
    "mongoose-concrete-timestamps": "0.1.0",
    "q": "1.5.0",
    "request": "2.81.0",
    "winston": "2.3.1",
    "x-ray": "2.3.2",
    "xml2js": "0.4.17"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "chai": "4.0.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "supertest": "0.10.0"
  }
}
